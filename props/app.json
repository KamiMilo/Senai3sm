{
  "expo": {
    "name": "props",
    "slug": "props",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
      "expo-font",
      {
        "fonts": "node_modules/@expo-google-fonts/poppins/Poppins_400Regular_Italic.ttf",
        "fonts": "node_modules/@expo-google-fonts/poppins/SingleDay_400Regular.ttf"
      }
      ]
    ]
  }
}
